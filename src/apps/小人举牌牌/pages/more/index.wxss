.listWrap {
}

.linklist {
    position: relative;
    width: 718rpx;
    margin: 16rpx;
    border: 1px solid #87CEFA;
    box-sizing: border-box;
    border-radius: 40rpx;
    padding: 30rpx 20rpx;
    overflow: hidden;
}

.linklist image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-right: 16rpx;
    float: left;
}

.title {
    display: flex;
    font-size: 36rpx;
    color: #333;
    margin-top: 12rpx;
}

.content {
    margin-top: 8rpx;
    font-size: 28rpx;
    line-height: 36rpx;
    color: #888;
}

.viewbtn {
    position: absolute;
    right: 16rpx;
    top: 65rpx;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 30rpx;
    font-size: 28rpx;
    border: 1px solid #87CEFA;
    color: #87CEFA;
    border-radius: 30rpx;
}

.copyright {
    position: absolute;
    width: 100%;
    text-align: center;
    bottom: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #ccc;
}

.backhome {
    position: relative;
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
}

.backhome image {
    width: 50rpx;
    height: 50rpx;
    float: left;
    margin-left: 8rpx;
    margin-top: 4rpx;
}

.backhome text {
    position: absolute;
    width: 100rpx;
    text-align: center;
    left: 50%;
    margin-left: -50rpx;
    font-size: 28rpx;
}

.backbtn {
    position: fixed;
    bottom: 60rpx;
    width: 120rpx;
    height: 120rpx;
    line-height: 120rpx;
    border-radius: 50%;
    left: calc(50% - 60rpx);
    color: #ffffff;
    background-color: #fdcf46;
    border-color: #ffffff;
    box-shadow: 0 0 10px #fdcf46;
    font-size: 16.0px;
    padding: 0px;
    border-width: 2px;
    border-style: solid;
    text-align: center;
}